{
  "name": "md-attr-parser",
  "version": "1.1.6",
  "description": "A parser for markdown's attributes",
  "main": "dist/index.js",
  "scripts": {
    "pretest": "xo",
    "prepare": "del-cli dist && cross-env BABEL_ENV=production babel src --out-dir dist",
    "test": "ava"
  },
  "repository": "github:arobase-che/md-attr-parser",
  "keywords": [
    "markdown",
    "attribute",
    "plugin"
  ],
  "author": "ache <ache@ache.one>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/arobase-che/md-attr-parser/issues"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "cross-env": "^5.1.4",
    "del-cli": "^1.1.0",
    "xo": "^0.23.0"
  },
  "xo": {
    "space": true,
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "no-var": "warn",
      "object-shorthand": "warn"
    }
  }
}
